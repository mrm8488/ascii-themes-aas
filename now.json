{
  "name": "ascii-themes-aas",
  "version": 2,
  "public": true,
  "regions": ["all"],
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node"
    },
    { "src": "themes/*/themes/*.json", "use": "@now/static" }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}
